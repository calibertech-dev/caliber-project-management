<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
  <fullName>Display_Name__c</fullName>
  <label>Display Name</label>
  <type>Text</type>
  <formulaTreatBlanksAs>BlankAsBlank</formulaTreatBlanksAs>
  <formula><![CDATA[
    IF(NOT(ISBLANK(Service_Address__c)), Service_Address__r.Name & " | ", "") &
    TEXT(Type__c)
  ]]></formula>
  <inlineHelpText>Auto-generated display name combining Service Address and Project Type.</inlineHelpText>
  <trackHistory>false</trackHistory>
  <trackTrending>false</trackTrending>
</CustomField>
